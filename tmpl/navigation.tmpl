<?js var self = this; ?>
<div class="navigation">
    <h2 class="applicationName"><a href="index.html"><?js= env.conf.templates.applicationName || 'Documentation' ?></a></h2>

    <div class="search">
        <input id="search" type="text" class="form-control input-sm" placeholder="Search Documentations">
    </div>

    <?js let showTypeTitle = Object.keys(this.nav).filter(type => this.nav[type].all.length > 0).length > 1; ?>

    <?js Object.keys(this.nav).forEach((type) => { ?>
        <?js if (this.nav[type].all.length) { ?>
        <div class="nav-type-group" data-type="<?js= type ?>">
            <?js if (showTypeTitle) { ?>
            <h3><?js= type.charAt(0).toUpperCase() + type.slice(1) ?></h3>
            <?js } ?>

            <?js Object.keys(this.nav[type].categories).forEach(category => { ?>
            <div class="nav-category-group">
                <h4><?js= category ?></h4>

                <?js= this.partial('navigation-list.tmpl', {
                    items: this.nav[type].categories[category],
                    self: this
                }) ?>
            </div>
            <?js }); ?>

            <?js if (this.nav[type].nocategory.length) { ?>
                <?js= this.partial('navigation-list.tmpl', {
                    items: this.nav[type].nocategory,
                    self: this
                }) ?>
            <?js } ?>

        </div>
        <?js } ?>
    <?js }); ?>

    <footer>
        Documentation generated by <a href="https://github.com/jsdoc3/jsdoc">JSDoc <?js= env.version.number ?></a> on <?js= (new Date()) ?>
    </footer>
</div>
